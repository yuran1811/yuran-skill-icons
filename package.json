{
  "name": "skill-icons",
  "description": "Showcase your skills on your GitHub or resumÃ© with ease!",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "scripts": {
    "build": "yarn build.icons-json && qwik build",
    "build.client": "vite build",
    "build.icons-json": "node ./src/scripts/build.js",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "clean": "rm -rf ./dist ./server ./tmp ./.vercel",
    "deploy": "vercel deploy --prod",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "svgo": "yarn dlx -p svgo svgo -rf public/icons --config ./.svgo.config.js",
    "vercel:dev": "vercel dev",
    "vercel:preview": "vercel deploy",
    "qwik": "qwik"
  },
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "trustedDependencies": [
    "sharp"
  ],
  "dependencies": {
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.12.0",
    "@builder.io/qwik-city": "^1.12.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "8.57.1",
    "@types/eslint": "8.56.12",
    "@types/node": "^20.17.17",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "8.57.1",
    "eslint-plugin-qwik": "^1.12.0",
    "globals": "^15.14.0",
    "prettier": "^3.5.0",
    "typescript": "5.7.3",
    "undici": "^7.3.0",
    "vercel": "^41.0.3",
    "vite": "5.4.14",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "yarn@4.6.0"
}
